<p>編集処理画面</p>

<%= form_with(model: @article, url: articles_update_path, method: :post, local: true) do |f| %>
    <div><%= f.label :title %></div>
    <div><%= f.text_field :title %></div><br>
    <div><%= f.label :kind %></div>
    <div><%= f.text_field :kind %></div><br>
    <div><%= f.label :dog_name %></div><br>
    <div><%= f.text_field :dog_name %><br>
    <div><%= f.label :body %></div>
    <div><%= f.text_field :body %></div><br>
    <div><%= f.label :image %></div>
    <div><%= f.text_field :image %></div><br>
    <div><%= f.submit '編集する' %></div>
<% end %>