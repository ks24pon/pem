<div class="container">
	<div class="row">
    <h1>投稿テスト</h1>
      <%= form_with(model: @article, url: articles_path, method: :post, local: true) do |f| %>
          <div><%= f.label :title %></div>
          <div><%= f.text_field :title %></div><br>
          <div><%= f.label :kind %></div>
          <div><%= f.text_field :kind %></div><br>
          <div><%= f.label :dog_name %></div>
          <div><%= f.text_field :dog_name %></div><br>
          <div><%= f.label :body %></div>
          <div><%= f.text_field :body %></div><br>
          <div><%= f.label :image %></div>
          <div><%= f.file_field :image, accept: 'image/*' %></div><br>
          <div><%= f.hidden_field :article_image_cache %>
          <div><%= f.submit '投稿する' %></div>
      <% end %>
	</div>
</div>
